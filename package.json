{
    "name": "hlink-dashb",
    "displayName": "Hyperlink VS Code Dashboard",
    "description": "Transforms the hardcoded VS Code watermark into interactive hyperlinks.",
  "version": "1.1.2612",
    "publisher": "14ag",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/14ag/hyperlink-vscode-extension"
    },
    "keywords": [
        "watermark",
        "hyperlink",
        "ui",
        "customization",
        "workbench"
    ],
    "license": "MIT",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "hyperlink-vscode-extension.patch",
                "title": "Apply Watermark Patch"
            },
            {
                "command": "hyperlink-vscode-extension.unpatch",
                "title": "Remove Watermark Patch (Uninstall)"
            }
        ]
    },
    "scripts": {
        "postinstall": "node patcher.js patch",
        "package:vsix": "vsce package --skip-license -o extension.vsix",
        "test:unit": "mocha --timeout 10000 \"test/suite/**/whitebox.test.js\" --bail",
        "test:integration": "node ./test/runTest.js",
        "test": "npm run test:unit && npm run test:integration"
    },
    "devDependencies": {
        "@vscode/test-electron": "^2.0.0",
        "mocha": "^10.2.0",
        "glob": "^8.1.0",
        "chai": "^4.3.7"
    }
}
